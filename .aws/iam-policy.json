{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3BucketAccessForStaticWebsite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListBucket",
        "s3:GetBucketLocation",
        "s3:GetBucketPolicy",
        "s3:PutBucketPolicy",
        "s3:GetBucketWebsite",
        "s3:PutBucketWebsite",
        "s3:GetBucketVersioning"
      ],
      "Resource": [
        "arn:aws:s3:::rohitaarav.me",
        "arn:aws:s3:::rohitaarav.me/*"
      ]
    }
  ]
}